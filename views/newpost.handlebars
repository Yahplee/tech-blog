<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="display-4">Fluid jumbotron</h1>
    <p class="lead">This is a modified jumbotron that occupies the entire horizontal space of its parent.</p>
  </div>
</div>

<div class="card text-center">
  <div class="card-header">
    Featured
  </div>
  <div class="card-body">
    <form class="createPost">
      <div class="form-group">
          <label for="createPostTitle">Email address</label>
          <input type="text" class="form-control" id="createPostTitle">
      </div>
      <div class="form-group">
          <label for="createPostContent">Password</label>
          <input type="textarea" class="form-control" id="createPostContent">
      </div>
      <button type="submit" class="btn btn-primary">Create</button>
    </form>
  </div>
  <div class="card-footer text-muted">
    2 days ago
  </div>
</div>

<script src="/js/newPost.js"></script>